<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:TrueFalse"
             x:Class="TrueFalse.MainPage">
    <ContentPage.Resources>
        <ResourceDictionary>

        </ResourceDictionary>
    </ContentPage.Resources>


    <StackLayout Margin="0,0,0,0">
        <Label x:Name="Questions" Text="Question" FontSize="Title"></Label>
        <StackLayout Orientation="Horizontal">
            <Button  Text="True" Clicked="True" x:Name="True_Button"></Button>
            <Button  Text="False" Clicked="False" x:Name="False_Button"></Button>
        </StackLayout>

    </StackLayout>

</ContentPage>
